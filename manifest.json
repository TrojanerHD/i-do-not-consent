{
  "manifest_version": 2,
  "name": "I Do Not Consent",
  "version": "0.0.3",
  "description": "Stop getting annoyed by “Do you consent” pop ups. But instead of just accepting them, reject them as much as possible",
  "content_scripts": [
    {
      "matches": [
        "*://consent.youtube.com/m*",
        "*://*.google.com/*"
      ],
      "js": [
        "idnc-dialog.js"
      ]
    },
    {
      "matches": [
        "*://consent.youtube.com/d*",
        "*://consent.google.com/d*"
      ],
      "js": [
        "idnc-check.js"
      ]
    }
  ]
}